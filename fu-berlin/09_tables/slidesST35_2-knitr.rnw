\section{Notwendigkeit statistischer Geheimhaltung}
\begin{frame}\frametitle{Notwendigkeit statistischer Geheimhaltung}
	\begin{itemize}
	    \item {\bf unterschiedliche Gründe} statistische Geheimhaltung (auch) in Tabellen zu betreiben\pause
	    \begin{itemize}
	        \item gesetzliche Vorgaben
	        \item Respondentenschutz \pause
	   	\end{itemize}
		\item {\bf Data privacy} ist ein wichtiges Thema\pause
	    \begin{itemize}
	        \item wem ''gehören'' meine Daten
	        \item kann ich mich sicher sein, dass Information nicht zu meinem Nachteil verwendet wird \pause
	   	\end{itemize}
		\item {\bf Kursziele:}
	    \begin{itemize}
	        \item Bewusst machen, wie Angreifer vorgehen
	        \item welche Möglichkeiten gibt es, (tabellarische) Daten zu schützen
	  	\end{itemize}
	\end{itemize}
\end{frame}

\begin{frame}\frametitle{Was versteht man unter ''Disclosure''?}
	\begin{itemize}
	    \item als {\bf Disclosure} versteht man, wenn aus veröffentlichten Daten Information über eine einzelne, spezifische statistische Einheit abgeleitet ("gelernt") werden kann.\pause
	    \item {\bf Beispiel 1: ( Ort = Gemeinde x; Fußballinteresse x Geschlecht) }:
		\begin{scriptsize}
		\begin{center}
			\begin{tabular}{|r|ll|l|}
			\hline
			& {\bf männlich} 		& {\bf weiblich} & {\bf Gesamt} \\ \hline
			{\bf Fußballfan} 		& 12 & 10 & {\bf 22}\\
			{\bf kein Fußballfan} 	& 93 & 85 & {\bf 178} \\ \hline
			{\bf Gesamt} 			& {\bf 105} & {\bf 95} & {\bf 200} \\ \hline
			\end{tabular}
		\end{center}
		\end{scriptsize} \pause
	    \item {\bf Beispiel 1 (cont.): ( Ort = Gemeinde x; Vereinspräferenz x Geschlecht) }:
		\begin{scriptsize}
		\begin{center}
			\begin{tabular}{|r|ll|l|}
			\hline
			& {\bf männlich} & {\bf weiblich} & {\bf Gesamt} \\ \hline
			{\bf Sk Rapid Wien} & 12 & 4 & {\bf 16}\\
			{\bf Sturm Graz} 	& 0 & 3 & {\bf 3} \\
			{\bf SV Ried} 		& 0 & 3 &  {\bf 3} \\ \hline
			{\bf Gesamt} 		& {\bf 12} & {\bf 10} & {\bf 22} \\ \hline
			\end{tabular}
		\end{center}
		\end{scriptsize}
	\end{itemize}
\end{frame}

\begin{frame}\frametitle{Was versteht man unter ''Disclosure''?}
	\begin{itemize}
	    \item als {\bf Disclosure} versteht man, wenn aus veröffentlichten Daten Information über eine einzelne, spezifische statistische Einheit abgeleitet ("gelernt") werden kann.
	    \item {\bf Beispiel 2: ( Ort = Gemeinde x; Bildung x Geschlecht) }:
		\begin{scriptsize}
		\begin{center}
			\begin{tabular}{|r|ll|l|}
			\hline
			& {\bf männlich} & {\bf weiblich} & {\bf Gesamt} \\ \hline
			{\bf keine/Pflichtschule} 	& 49 & 53 & {\bf 102}\\
			{\bf Lehre} 				& 34 & 23 & {\bf 56} \\
			{\bf AHS/BHS} 				& 22 & 14 &  {\bf 37} \\
			{\bf Universitäre Bildung} 	&  0 &  5 & {\bf 5} \\ \hline
			{\bf Gesamt} 				& {\bf 105} & {\bf 95} & {\bf 200} \\ \hline
			\end{tabular}
		\end{center}
		\end{scriptsize} \pause
	  	\item man unterscheidet {\bf verschiedene Arten von Disclosure}, z.B: \pause
	  	\begin{itemize}
	  		\item Group disclosure (Beispiel 1)
	  		%\item Attribute disclosure
	  		\item (Negative) attribute disclosure (Beispiel 2)
	  		\item spontane Erkennung (z.B bei Personen mit seltenen Merkmalskombinationen)
	  	\end{itemize}
	\end{itemize}
\end{frame}

\section{Methoden zur Geheimhaltung tabellarischer Daten}
\subsection{Der Aufbau statistischer Tabellen}
\begin{frame}\frametitle{Tabellen - Grundsätzliches}
	\begin{itemize}
	    \item Grundlage für alle Tabellen sind: {\bf Mikrodaten} \pause
	    \item Unterscheidung von {\bf Wertetabellen} und {\bf Häufigkeitstabellen} \pause
	    \begin{itemize}
	    	\item {\bf Häufigkeitstabelle:} Anzahl der beitragenden Einheiten für jeder Zelle der Tabelle wird ausgewiesen. \pause
	    	\item {\bf Wertetabelle:} Für eine erhobene Variable wird die Summe dieser Variable über alle beitragenden Einheiten in jeder Zelle der Tabelle ausgewiesen. \pause
	    \end{itemize}
	    \item {\bf Grundsätzlich...}
	    \begin{itemize}
	    	\item sind Tabellen gekennzeichnet durch {\bf lineare Abhängigkeiten} zwischen Zellen. \pause
	    	\item können statistische Tabellen {\bf ein- oder mehrdimensional}, {\bf hierarchisch} und/oder {\bf verlinkt} sein.
		\end{itemize}
	\end{itemize}
\end{frame}


